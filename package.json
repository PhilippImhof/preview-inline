{
  "name": "preview-inline",
  "main": "./lib/preview-inline",
  "version": "1.1.1",
  "description": "Inline preview of LaTeX maths and images in markdown files",
  "keywords": [
    "markdown",
    "commonmark",
    "pandoc",
    "latex",
    "image",
    "preview",
    "inline",
    "math",
    "block"
  ],
  "repository": "https://github.com/mangecoeur/preview-inline",
  "license": "MIT",
  "engines": {
    "atom": ">=1.6.0 <2.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^3.7.0",
    "eslint-config-google": "^0.6.0"
  },
    "line_endings": {
      "value": "unix",
      "level": "error"
    },
    "max_line_length": {
      "value": 120,
      "level": "warn"
    }
}
