{
  "name": "preview-inline",
  "main": "./lib/preview-inline",
  "version": "1.0.1",
  "description": "Inline preview of LaTeX maths and images in markdown files",
  "keywords": [
    "markdown",
    "latex",
    "image",
    "preview",
    "inline"
  ],
  "repository": "https://github.com/mangecoeur/preview-inline",
  "license": "MIT",
  "engines": {
    "atom": ">=1.6.0 <2.0.0"
  },
  "dependencies": {},
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "value": "unix",
      "level": "error"
    },
    "max_line_length": {
      "value": 120,
      "level": "warn"
    }
  }
}
