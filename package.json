{
  "name": "preview-inline",
  "main": "./lib/preview-inline",
  "version": "1.5.2",
  "description": "Inline preview of LaTeX maths, TikZ and images in markdown files",
  "keywords": [
    "preview",
    "math",
    "image",
    "markdown",
    "latex",
    "commonmark",
    "inline",
    "productivity",
    "academic"
  ],
  "repository": "https://github.com/mangecoeur/preview-inline",
  "license": "MIT",
  "engines": {
    "atom": ">=1.22.0 <2.0.0"
  },
  "dependencies": {
    "@atom/temp": "^0.8.4",
    "babel-preset-es2015": "^6.24.1",
    "untildify": "^3.0.3"
  },
  "devDependencies": {
    "eslint": "^3.7.0",
    "eslint-config-google": "^0.6.0"
  },
  "line_endings": {
    "value": "unix",
    "level": "error"
  },
  "max_line_length": {
    "value": 120,
    "level": "warn"
  },
  "consumedServices": {
    "bibliographicReferences": {
      "versions": {
        "1.0.0": "getReferences"
      }
    }
  }
}
